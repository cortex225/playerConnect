{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "regions": ["cdg1"],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODEJS_VERSION": "20.x",
    "PNPM_VERSION": "10.x"
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_APP_URL": "@next_public_app_url"
    }
  }
}
